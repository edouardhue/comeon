<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.4.0 http://maven.apache.org/xsd/decoration-1.4.0.xsd"
         name="ComeOn!">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.5</version>
    </skin>
    <body>
        <menu ref="reports"/>
    </body>
    <custom>
        <fluidoSkin>
            <gitHub>
                <projectId>edouardhue/comeon</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>red</ribbonColor>
            </gitHub>
            <piwik>
                <piwikUrl>analytics.wikimedia.fr</piwikUrl>
                <idsite>11</idsite>
            </piwik>
            <profile>pre-release</profile>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>false</sideBarEnabled>
            <topBarEnabled>true</topBarEnabled>
            <topBarIcon>
                <name>ComeOn!</name>
                <alt>ComeOn! logo</alt>
                <src>/comeon_32_32.png</src>
                <href>/index.html</href>
            </topBarIcon>
        </fluidoSkin>
    </custom>
</project>